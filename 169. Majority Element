class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int majority;
        int cnt = 0;
        for(int i=0;i<nums.size();++i){
            if(cnt==0){
                majority=nums[i];
                cnt++;
            }
            else {
                majority==nums[i]?cnt++:cnt--;
                if(cnt>=nums.size()/2+1)
                    return majority;
            }
        }
        return majority;
    }
};// Mooreâ€™s Voting Algorithm;
